{
  "name": "unreleasd-presence-helper",
  "version": "1.0.0",
  "description": "Electron helper for Discord Rich Presence integration",
  "main": "dist/main.js",
  "type": "commonjs",
  "scripts": {
    "dev": "tsc && electron .",
    "build:app": "tsc",
    "build": "npm run build:app && electron-builder --config builder.config.json",
    "pack": "npm run build:app && electron-builder --config builder.config.json --dir",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "electron",
    "discord",
    "rich-presence"
  ],
  "author": "Unreleasd",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "electron": "^30.0.0",
    "electron-builder": "^24.13.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@xhayper/discord-rpc": "^1.1.2",
    "dotenv": "^16.4.0",
    "zod": "^3.22.0"
  }
}
